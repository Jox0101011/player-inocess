_plugin() {
    cat ../plugins/*.* | grep PLUGIN_NAME=
  }
_install() {
   sudo pacman -S npm
   sudo npm -g live-server
  }

_web() {

  live-server web/

  echo -e "[+] ON"
  }

player() {
  case "$1" in
  web) _web ;;
  install) _install ;;
  plugin) _plugin ;;
  *) echo "\e[31m[!] Usage: player [web|install|plugin]\e[0m" ;;

  esac
  }
