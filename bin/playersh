source 
load_plugins

playersh() {

    while true; do
        read -e -p $"Player >" input
    [[ -n "$input" ]];

  cmd="${input%% *}"
  args="${input#* }"

  if declare -f "$cmd" > /dev/null; then
    $cmd $args
  elif command -v "$cmd" > /dev/null; then
    $cmd $args 
  else
    echo "Command '$cmd' not available."
  fi
done
  }
